<div class="row">
    <div class="col s12 m6">
      <div class="row">
        <div class="input-field col s12">
            <%= form_with url: "/books/new" , :method => 'get' do |f| %>
              <%= f.text_field :form_word ,class: "autocomplete books-input", placeholder: 'タイトル・著者で検索' %>
                  <%= button_tag type: "submit" do %>
                  <i class="material-icons">search</i>
              <% end %>
            <% end %>
        </div>
      </div>
    </div>
</div>
<div class="row" id="book-search-result">
</div>
